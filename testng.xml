<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
 
<suite name="API TestSuite" thread-count="5" parallel="tests">
  
  <listeners>
  	<listener class-name="utils.ExtentReporter" />
  </listeners>
  
  <test name="User Tests">
    <classes>
      <class name="testcases.UserTests"/>
    </classes>
  </test>
  
  <test name="cart Tests">
    <classes>
      <class name="testcases.CartTests"/>
    </classes>
  </test>
  
  <test name="login Tests">
    <classes>
      <class name="testcases.LoginTests"/>
    </classes>
  </test>
  
  
  
   <test name="Product Tests">
    <classes>
      <class name="testcases.ProductTests"/>
    </classes>
  </test>
  
 
  
</suite>